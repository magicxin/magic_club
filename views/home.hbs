	
	<div class="content">
	{{#each articles}}
		<div class="article_item">
			<div class="left">
				<div class="avatar">
					<div class="img">
					
					</div>
				</div>
				<div class="nickname">
					{{nickname}}
				</div>
			</div>
			<div class="right">
				<div class="title">
					<h3>{{title}}</h3>
				</div>
				<div class="tabs">
					<div class="updatetime">
						{{updatetime}}
					</div>
					<div class="goodidea">
						<img src="/img/goodidea.png" />{{goodidea}}
					</div>
				</div>
			</div>
		</div>
	{{/each}}
	</div>
	