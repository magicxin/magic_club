	<div class="content">
		<div class="content_left">
		{{#each articles}}
			<div class="article_item">
				<div class="left">
					<div class="avatar">
						<div class="img">
						
						</div>
					</div>
					<div class="nickname">
						{{author}}
					</div>
				</div>
				<div class="middle">
					<div class="title">
						<h3>{{title}}</h3>
					</div>
					<div class="tabs">
						<div class="updatetime">
							{{updatetime}}
						</div>
						<div class="goodidea">
							<img src="{{avatar}}" />{{goodidea}}
						</div>
					</div>
				</div>
			</div>
		{{/each}}
		</div>
		<div class="content_right">
			<div class="blog">
				<div class="my_blog">
					<div class="my_blog_img"><a>我的文章</a></div>
				</div>
				<div class="write_blog">
					<div class="write_blog_img"><a href="/write_blog">写新文章</a></div>
			    </div>
			</div>
			<div class="blog">
				<div class="my_blog">
					<div class="my_blog_img"><a>我的文章</a></div>
				</div>
				<div class="write_blog">
					<div class="write_blog_img"><a>我的文章</a></div>
				</div>
			</div>
		</div>
	</div>